public class Main {


    public static void main(String[] args) {

        int [] arr = {12, 24, 48, 6, 44, 5664, 4344, 566, 568, 13} ;
        System.out.println(neggativeTrue(arr));
        }

        static boolean neggativeTrue (int[] arr) {
        for (int i = 0; i < arr.length ; i++) {
            if (arr[i] % 2 != 0)
                return true ;

            }
        return false ;
            }



        }
