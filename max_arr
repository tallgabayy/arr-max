public class Main {


    public static void main(String[] args) {
int [] arr = {12, 24, 45, 6, 45, 5664, 4344, 566, 569, 111} ;
        }

        static int max (int[] arr) {
        int max = arr[0] ;
        for (int i = 1; i < arr.length; i++) {
           if (arr[i] > max);
           max = arr[i];
        }
         return max;

        }

    }
