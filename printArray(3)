public class Main {


    public static void main(String[] args) {

        int[] arr = {12, 24, 48, 6, 44, 5664, 4344, 566, 568, 13};
        printArray(arr);
    }

    static void printArray(int[] arr) {
        System.out.print("{");
        for (int i = 0; i < arr.length - 1; i++) {
            System.out.print(arr[i] + ", ");
        }
        if (arr.length > 0)
            System.out.print(arr[arr.length - 1]);
        System.out.println("}");
    }

}
