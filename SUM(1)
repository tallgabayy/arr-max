public class Main {


    public static void main(String[] args) {

        int [] arr = {12, 24, 45, 6, 45, 5664, 4344, 566, 569, 111} ;
        System.out.println(sum(arr));
        }

        static int sum (int[] arr) {
        int sum = 0 ;
        for (int i = 0; i < arr.length; i++) {
        sum += arr[i];
            }

         return sum ;

        }

    }
